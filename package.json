{
  "name": "@raviqqe/hidash",
  "version": "2.0.3",
  "description": "Yet another extension to Lodash",
  "repository": "https://github.com/raviqqe/hidash",
  "license": "MIT",
  "publishConfig": {
    "provenance": true
  },
  "engines": {
    "node": ">=24.6.0"
  },
  "type": "module",
  "exports": {
    "./*.js": {
      "types": "./dist/*.d.ts",
      "default": "./dist/*.js"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "default": "./dist/*.js"
    }
  },
  "scripts": {
    "build": "npm run build:tsc && npm run build:esbuild",
    "build:esbuild": "esbuild --minify --format=esm --sourcemap --outdir=dist $(git ls-files src)",
    "build:tsc": "tsc --emitDeclarationOnly",
    "lint": "eslint src",
    "test": "vitest --coverage"
  },
  "dependencies": {
    "@raviqqe/loscore": "^0.2.14"
  },
  "devDependencies": {
    "@raviqqe/eslint-config": "^5.0.0",
    "@raviqqe/tsconfig": "^2.0.1",
    "@vitest/coverage-v8": "^3.2.4",
    "esbuild": "^0.25.9",
    "eslint": "^9.34.0",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "packageManager": "pnpm@10.15.0+sha512.486ebc259d3e999a4e8691ce03b5cac4a71cbeca39372a9b762cb500cfdf0873e2cb16abe3d951b1ee2cf012503f027b98b6584e4df22524e0c7450d9ec7aa7b"
}
