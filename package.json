{
  "name": "@raviqqe/hidash",
  "version": "2.0.3",
  "description": "Yet another extension to Lodash",
  "repository": "https://github.com/raviqqe/hidash",
  "license": "MIT",
  "publishConfig": {
    "provenance": true
  },
  "type": "module",
  "exports": {
    "./*.js": {
      "types": "./dist/*.d.ts",
      "default": "./dist/*.js"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "default": "./dist/*.js"
    }
  },
  "scripts": {
    "build": "npm run build:tsc && npm run build:esbuild",
    "build:esbuild": "esbuild --minify --format=esm --sourcemap --outdir=dist $(git ls-files src)",
    "build:tsc": "tsc --emitDeclarationOnly",
    "prepare": "npm run build",
    "lint": "eslint src",
    "test": "vitest --coverage"
  },
  "dependencies": {
    "@raviqqe/loscore": "^0.2.0"
  },
  "devDependencies": {
    "@raviqqe/eslint-config": "^4.0.2",
    "@raviqqe/tsconfig": "^1.0.1",
    "@vitest/coverage-v8": "^2.0.1",
    "esbuild": "^0.23.0",
    "eslint": "^8.34.0",
    "typescript": "^5.0.2",
    "vitest": "^2.0.1"
  }
}
